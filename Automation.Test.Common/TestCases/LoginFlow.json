{
  "TestCaseId": "LOGIN_001",
  "TestName": "Admin Login Flow",
  "Page": "LoginPage",
  "Steps": [
    {
      "Action": "Click",
      "Target": "OpenLoginButton"
    },
    {
      "Action": "SelectCombo",
      "Target": "AccessTypeCombo",
      "Value": "Admin"
    },
    {
      "Action": "SetText",
      "Target": "PasswordTextBox",
      "Value": "Admin@123"
    },
    {
      "Action": "Click",
      "Target": "ConfirmLoginButton"
    },
    {
      "Action": "ValidateText",
      "Target": "LoginStatusLabel",
      "Expected": "Logged In"
    },
    {
      "Action": "WaitForVisible",
      "Target": "LoginPopup",
      "TimeoutMs": 5000
    }
  ]
}
